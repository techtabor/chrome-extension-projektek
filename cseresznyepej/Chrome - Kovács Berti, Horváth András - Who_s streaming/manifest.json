{
  "name": "Who's streaming?",
  "description": "Who is streaming at the moment? Find out with this revolutionary Chrome Extension!",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false,
    "matches": ["<all_urls>"]
  },
  "permissions": [
        "http://*/*",
        "https://*/*",
        "notifications",
        "storage",
        "tabs"
      ],
  "web_accessible_resources": [
        "https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"
    ],
"content_security_policy": "script-src 'self' https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js ; object-src 'self'",
"options_page": "options.html",
"options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
      "default_icon": "icon_128.png",
      "default_popup":"popup.html"
  },
  "manifest_version": 2,
  "icons": {
        "16": "icon_128.png",
        "48": "icon_128.png",
        "128": "icon_128.png"
    }
} 