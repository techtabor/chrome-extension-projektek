{
  "name": "Twitch Emotes",
  "description": "Spices up the internet with emotes from Twitch!",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_icon": "leKappa.png",
      "default_title": "Twitch Emotes",
      "default_popup": "menu.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "<all_urls>"
  ],
  "manifest_version": 2
} 