{
    "manifest_version": 2,
  
    "name": "Fast Downloader",
    "description": "This extension will help you to download image faster",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "tabs",
     "downloads"
     ],
     "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.js",
                "content_script.js"
            ],
            "css": [
                "css/content.css"
            ]
            
        }
    
    ]
}


       